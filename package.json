{
  "name": "@alexzzz/nl",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.14.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/alexzhang1030/nl#readme",
  "bugs": {
    "url": "https://github.com/alexzhang1030/nl/issues"
  },
  "license": "MIT",
  "author": "alexzhang1030",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/nl.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp package.json --commit \"chore: release v%s\" --push --tag && pnpm -r publish --access public",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "ora": "^6.1.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@alexzzz/eslint-config": "1.4.0",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/cli-progress": "3.11.0",
    "@types/node": "17.0.45",
    "bumpp": "8.2.1",
    "eslint": "8.25.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "pnpm": "7.14.0",
    "tsx": "3.10.1",
    "typescript": "4.8.4",
    "unbuild": "0.9.4",
    "unplugin-auto-import": "0.11.2",
    "vitest": "0.24.1"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
