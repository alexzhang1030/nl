{
  "name": "@alexzzz/nl",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.3.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/developer-plus/pkg-name#readme",
  "bugs": {
    "url": "https://github.com/developer-plus/pkg-name/issues"
  },
  "license": "MIT",
  "author": "developer-plus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developer-plus/pkg-name.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && npm publish --access public",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "ora": "^6.1.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@alexzzz/eslint-config": "1.3.0",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/cli-progress": "3.11.0",
    "@types/node": "17.0.45",
    "bumpp": "7.2.0",
    "eslint": "8.18.0",
    "husky": "8.0.1",
    "lint-staged": "12.5.0",
    "pnpm": "7.3.0",
    "tsx": "3.5.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "unplugin-auto-import": "0.7.2",
    "vitest": "0.12.10"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
